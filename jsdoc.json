{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "include": ["README.md", "./src/store.js", "./src/indexer.js", "./src/walker.js"]
    },
    "plugins": ["plugins/markdown"],
    "sourceType": "module",
    "opts": {
        "readme": "./README.md",
        "recurse": true,
        "destination": "./docs",
        "template": "node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "default_theme": "light"
        }
    },
    "markdown": {
        "hardwrap": false,
        "idInHeadings": true
    }
}
